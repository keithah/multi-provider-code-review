{
  "version": "2.1.0",
  "$schema": "http://json.schemastore.org/sarif-2.1.0.json",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "multi-provider-code-review",
          "version": "2.0.0",
          "informationUri": "https://github.com/keithah/multi-provider-code-review",
          "rules": [
            {
              "id": "RULE-1",
              "shortDescription": {
                "text": "LLM finding"
              },
              "fullDescription": {
                "text": "llm"
              },
              "defaultConfiguration": {
                "level": "warning"
              }
            },
            {
              "id": "RULE-2",
              "shortDescription": {
                "text": "Console logging left in code"
              },
              "fullDescription": {
                "text": "Remove debug logging before merging."
              },
              "defaultConfiguration": {
                "level": "note"
              }
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "RULE-1",
          "level": "warning",
          "message": {
            "text": "llm"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/app.ts"
                },
                "region": {
                  "startLine": 5
                }
              }
            }
          ]
        },
        {
          "ruleId": "RULE-2",
          "level": "note",
          "message": {
            "text": "Remove debug logging before merging."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/app.ts"
                },
                "region": {
                  "startLine": 2
                }
              }
            }
          ]
        }
      ]
    }
  ]
}